{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "Webpack starter build for Front-End",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "build": "webpack --config ./config/webpack.config.js --env.NODE_ENV=development --mode=development --progress",
    "dev-network": "webpack-dev-server --config ./config/webpack.config.js --env.NODE_ENV=development --mode=development --progress --open --host 0.0.0.0",
    "dev": "webpack-dev-server --config ./config/webpack.config.js -env.NODE_ENV=development --mode=development --progress --open",
    "prod": "webpack --config ./config/webpack.config.js --env.NODE_ENV=production --mode=production -p --progress"
  },
  "author": "dotBraind",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "pug": "^3.0.1",
    "pug-loader": "^2.4.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-notifier": "^1.8.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.3.3",
    "bootstrap": "^4.4.1",
    "jquery": "^3.5.0"
  }
}
